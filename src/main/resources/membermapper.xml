<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.soldesk.order.login.MapperLogin">

<select id="getMember" parameterType="String" resultType="int">
select * from member where mem_id = #{mem_id}
</select>

<insert id="insertMember" parameterType="com.soldesk.order.login.MemberVO">
insert into member values (#{mem_id}, #{mem_name}, #{mem_gender}, #{mem_email},#{mem_age})
</insert>

<!-- <delete id=""></delete> -->



</mapper>